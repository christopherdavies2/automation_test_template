{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "createdAt",
    "name",
    "isPartTime",
    "locationId",
    "favLanguages",
    "address"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "string",
      "title": "The Id Schema",
      "default": "",
      "examples": [
        "1"
      ],
      "pattern": "^(.*)$"
    },
    "createdAt": {
      "$id": "#/properties/createdAt",
      "type": "string",
      "title": "The Createdat Schema",
      "default": "",
      "examples": [
        "2019-03-09T20:15:02.098Z"
      ],
      "pattern": "^(.*)$"
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "Norbert Wiza"
      ],
      "pattern": "^(.*)$"
    },
    "isPartTime": {
      "$id": "#/properties/isPartTime",
      "type": "boolean",
      "title": "The Isparttime Schema",
      "default": false,
      "examples": [
        false
      ]
    },
    "locationId": {
      "$id": "#/properties/locationId",
      "type": "integer",
      "title": "The Locationid Schema",
      "default": 0,
      "examples": [
        16
      ]
    },
    "favLanguages": {
      "$id": "#/properties/favLanguages",
      "type": "array",
      "title": "The Favlanguages Schema",
      "items": {
        "$id": "#/properties/favLanguages/items",
        "type": "string",
        "title": "The Items Schema",
        "default": "",
        "examples": [
          "java",
          "ruby",
          "python"
        ],
        "pattern": "^(.*)$"
      }
    },
    "address": {
      "$id": "#/properties/address",
      "type": "object",
      "title": "The Address Schema",
      "required": [
        "street",
        "city",
        "postcode"
      ],
      "properties": {
        "street": {
          "$id": "#/properties/address/properties/street",
          "type": "string",
          "title": "The Street Schema",
          "default": "",
          "examples": [
            "11 A street"
          ],
          "pattern": "^(.*)$"
        },
        "city": {
          "$id": "#/properties/address/properties/city",
          "type": "string",
          "title": "The City Schema",
          "default": "",
          "examples": [
            "Leeds"
          ],
          "pattern": "^(.*)$"
        },
        "postcode": {
          "$id": "#/properties/address/properties/postcode",
          "type": "string",
          "title": "The Postcode Schema",
          "default": "",
          "examples": [
            "LS1 1A1"
          ],
          "pattern": "^(.*)$"
        }
      }
    }
  }
}